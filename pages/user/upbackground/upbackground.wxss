/* pages/user/upbackground/upbackground.wxss */
.button {
    position: absolute;
}

.custom-class {
    width: 250rpx;
    margin-left: 250rpx;
    margin-right: 250rpx;
}

.imagesbox {
    width: 750rpx;
    height: 100vh;
    display: flex;
    align-items: center;
}

.imagesbox image {
    width: 750rpx;
}

.MenuBottom {
    width: 750rpx;
    position: absolute;
    /* background-color: ivory; */
    display: flex;
    z-index: 1;
}

.title {
    text-align: center;
    font-weight: bold;
    color: rgb(95, 95, 95);
    position: absolute;
    bottom: 0;
}
.scrollview{
    height: inherit;
}
.images{
    width: 730rpx;
    margin: 10rpx;
}
.updateTime{
    width: 750rpx;height: 50rpx;line-height: 50rpx;
    text-align: center;
    font-size: xx-small;
    color: rgb(32, 32, 32);
}
.end {
    width: 750rpx;
    height: 120rpx;
    /* background-color: #fff; */
    line-height: 120rpx;
    text-align: center;
    float: left;
    color: #ffaaaa;
}
.icon_clear{
    width: 50rpx;
    height: 50rpx;
    position: relative;
    top: 10rpx;
}
.icon_clear:active{
    width: 60rpx;
    height: 60rpx;
    top: 10rpx;
}