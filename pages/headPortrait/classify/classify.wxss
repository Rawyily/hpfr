/* pages/headPortrait/classify/classify.wxss */
page{
    background: rgb(238, 238, 238);
}
.swiper {
    width: 100vw;
    height: 100vh;
}

.title {
    width: 750rpx;
    position: absolute;
    top: 0;
    z-index: 1;
    background: rgb(238, 238, 238);
}
.title .titletext{
    position: absolute;
    bottom: 0;
    text-align: center;
    font-weight: bold;
    /* background-color: rgb(255, 0, 0); */
}
.scrollview {
    width: 750rpx;
    padding-left: 15rpx;
    padding-right: 15rpx;
    /* background-color: rgb(255, 195, 195); */
}
.imgbox {
    float: left;
    width: 220rpx;
    height: 220rpx;
    margin: 10rpx;
}

.imgbox .img {
    width: 220rpx;
    height: 220rpx;
    border-radius: 10rpx;

}
.loaded {
    width: 750rpx;
    height: 120rpx;
    /* background-color: #fff; */
    line-height: 120rpx;
    text-align: center;
    float: left;
    color: #ffaaaa;
}
.mp-loading{
    width: 250rpx;
    position: absolute;
    top: 50%;
    left: 250rpx;
}